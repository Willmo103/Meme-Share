  <img id=\"navbar-logo\" src=\"{{ url_for('static', filename='images/intragram_logo_light.png') }}\"\r\n                        height=\"32\" width=\"32\" alt=\"Site Icon\" class=\"mr-2\">\r\n                    <span class=\"text-2xl\">&nbsp;{{ title }}</span>\r\n                </a>\r\n            </div>\r\n            <div class=\"flex w-full pt-2 content-center justify-between md:w-1/2 md:justify-end\">\r\n                <ul class=\"list-reset flex justify-between flex-1 md:flex-none items-center\">\r\n                    <!-- Check if user is authenticated -->\r\n                    {% if current_user.is_authenticated %}\r\n                    <li class=\"mr-3\">\r\n                        <a class=\"inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4\"\r\n                            href=\"#\">Profile</a>\r\n                    </li>\r\n                    <li class=\"mr-3\">\r\n                        <a class=\"inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4\"\r\n                            href=\"#\">Logout</a>\r\n                    </li>\r\n                    {% else %}\r\n                    <li class=\"mr-3\">\r\n                        <a class=\"inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4\" class=\"nav-link\" href=\"{{ url_for('routes.login') }}\"\r\n                            >Login</a>\r\n                    </li>\r\n                    {% endif %}\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </nav>\r\n    <!-- Main Content -->\r\n    <div class=\"container mx-auto flex flex-wrap py-6\">\r\n        {% block content %}{% endblock %}\r\n    </div>\r\n</body>\r\n\r\n</html>\r\n","index.html":"{% extends 'base.html' %}\r\n{% block content %}\r\n    <h1>Index Page</h1>\r\n{% endblock %}\r\n","login.html":"{% extends \"base.html\" %}\r\n\r\n{% block content %}\r\n<div class=\"w-full max-w-xs mx-auto mt-20\">\r\n    <form id=\"userForm\" class=\"bg-white shadow-md rounded px-8 pt-6 pb-8 mb-4\" method=\"POST\">\r\n        <div class=\"mb-4\">\r\n            <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"username\">\r\n                Username\r\n            </label>\r\n            <input\r\n                class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\"\r\n                id=\"username\" type=\"text\" placeholder=\"Username\" name=\"username\" required>\r\n        </div>\r\n        <div class=\"mb-6\">\r\n            <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"password\">\r\n                Password\r\n            </label>\r\n            <input\r\n                class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline\"\r\n                id=\"password\" type=\"password\" placeholder=\"******************\" name=\"password\" required>\r\n        </div>\r\n        <div class=\"mb-6 hidden\" id=\"emailDiv\">\r\n            <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"email\">\r\n                Email\r\n            </label>\r\n            <input\r\n                class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline\"\r\n                id=\"email\" type=\"email\" placeholder=\"Email\" name=\"email\">\r\n        </div>\r\n        <div class=\"mb-6 hidden\" id=\"confirmPasswordDiv\">\r\n            <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"confirm_password\">\r\n                Confirm Password\r\n            </label>\r\n            <input\r\n                class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 mb-3 leading-tight focus:outline-none focus:shadow-outline\"\r\n                id=\"confirm_password\" t